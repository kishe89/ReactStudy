<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <script src="https://unpkg.com/react@latest/dist/react.js"></script>
    <script src="https://unpkg.com/react-dom@latest/dist/react-dom.js"></script>
    <script src="https://unpkg.com/babel-standalone@6.15.0/babel.min.js"></script>

    <script type="text/babel">
        ReactDOM.render(
                React.DOM.h1(
                        {id:"my-heading"},
                        React.DOM.span(null,
                        React.DOM.em(null,"Hell"),
                        "o"
                        ),
                        "world!"
                ),
                document.getElementById('root')
        )
        ReactDOM.render(
                <h1 id="my-sub-heading">
                    <span><em>Hell</em>o</span> world!
                </h1>,
                document.getElementById('subroot')
        )
        ReactDOM.render(
                <h1 id="a-tag-text">
                    <a href="/example2">example2</a>로 이동
                </h1>,
                document.getElementById('atagroot')
        )
        ReactDOM.render(
            React.DOM.h1(
                    {
                        id:"styleheading",
                        style:{
                            background:"black",
                            color:"white",
                            fontFamily:"Verdana",
                        }
                    },
                    "Hello Style!"
            ),
                document.getElementById('styleroot')
        )
    </script>
</head>
<body>
    <div id="root"></div>
    <div id="subroot"></div>
    <div id="atagroot"></div>
    <div id="styleroot"></div>
</body>
</html>